    "eslint": "^8.0.0",
    "eslint-plugin-svelte": "^2.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "vite": "^4.5.0"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "firebase": "^10.0.0"
  }
